{
    "name":"otakusakusaku",
    "version":"1.0",
    "manifest_version":2,

    "description":"TokyoOtakuMode(otakumode.com)内で使用可能なキーボードショートカットを追加",

    "permissions":[
        "http://otakumode.com/*"
    ],

    "content_scripts":[
        {
            "matches":["http://otakumode.com/*"],
            "js":["js/jquery-2.0.0.min.js","js/otasaku.js","js/content.js"],
            "css":["css/style.css"]
        }
    ]
}
